        <div class="containerForm">
            <div style="text-align:center">
                <h2 class="h2form">Contacto</h2>
                <form id="formContacto" action="" class="form">
                    <label class="lblform" for="fname">Nombre <i class="fa fa-user" aria-hidden="true"></i> </label>
                    <input type="text" id="fname" name="nombre" placeholder="Ingrese el nombre...">
                    <label class="lblform" for="lname">Email <i class="fa fa-envelope" aria-hidden="true"></i></label>
                    <input type="email" id="formail" name="email" placeholder="Ingrese un Email">
                    <label class="lblform" for="mensaje">Mensaje <i class="fa fa-edit" aria-hidden="true"></i></label>
